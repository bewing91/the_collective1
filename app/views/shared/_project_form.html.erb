<%= form_for(@project) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_field :description, placeholder: "What is it all about?" %>

    <%= f.submit "Create Project", class: "btn btn-primary" %>
<% end %>